<!DOCTYPE html>
<body>
    <div>
          <!-- <audio controls="" autoplay="" name="media">
            <source src="/audio?v=<%= locals.videoQuery %>" type="audio/ogg">
        </audio> -->
        <img src="<%=videoDetails['thumbnails']['high']['url']%>">
        <audio controls="" autoplay="" onloadstart="this.volume=0.1" name="media">
            <source src="<%= audioUrl %>" type="audio/ogg">
        </audio>
        <h2><%=videoDetails['title']%></h2>
        <img src="data:image/png;base64,"/>
        
        <% var previewJson = JSON.parse(preview) %>
        <% if ( locals.preview) { %>
            <% for(key in previewJson['video']) { %>
                <% if(previewJson['video'].hasOwnProperty(key)) { //make another for loop for all the keys%> 
                    <div>
                    <a href="/play?v=<%= previewJson['video'][key]['id'] %>">
                    <p><%= previewJson['video'][key]['title'] %></p>
                    <img src="<%= previewJson['video'][key]['thumbnail'] %>">
                    </a> 
                    </div>
                <% } %>
            <% } %>
        <% } %>
    </div>
</body>
</html>